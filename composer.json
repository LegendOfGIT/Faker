{
  "name": "legendofgit/faker",
  "description": "Faker is a PHP library that generates fake data for you.",
  "keywords": [
    "faker",
    "fixtures",
    "data"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcel Marschner"
    }
  ],
  "type": "library",
  "require": {
    "php": "^5.6 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35 || ^5.7"
  },
  "autoload": {
    "psr-4": {
      "Faker\\": "src/Faker"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ],
    "psr-4": {
      "Faker\\": "tests/Faker"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "optimize-autoloader": true
}
